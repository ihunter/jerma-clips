{
  "name": "twitch-clip-search-nuxt",
  "version": "1.0.0",
  "description": "Search twitch clips by title, game, and date",
  "author": "Ian Hunter",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
